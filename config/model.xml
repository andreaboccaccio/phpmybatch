<!DOCTYPE model>
<model>
	<class id="cause">
		<dbtab name="CAUSE" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="in_out" kind="string" value="" dbName="in_out" />
		<var name="name" kind="string" value="" dbName="name" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
	<class id="document">
		<dbtab name="DOCUMENT_DENORM" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="kind" kind="string" value="" dbName="kind" />
		<var name="code" kind="string" value="" dbName="code" />
		<var name="contractor_code" kind="string" value="" dbName="contractor_code" />
		<var name="contractor" kind="string" value="" dbName="contractor" />
		<var name="date" kind="stringDate" value="" dbName="doc_date" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
	<class id="item">
		<dbtab name="ITEM_DENORM" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="document" kind="int" value="-1" dbName="document" />
		<var name="kind" kind="string" value="" dbName="kind" />
		<var name="batch" kind="string" value="" dbName="batch" />
		<var name="batch_orig" kind="string" value="" dbName="batch_orig" />
		<var name="country" kind="int" value="-1" dbName="country" />
		<var name="district" kind="string" value="" dbName="district" />
		<var name="stabCEE" kind="string" value="" dbName="stabCEE" />
		<var name="qty" kind="int" value="-1" dbName="qty" />
		<var name="kg" kind="float" value="-1" dbName="kg" />
		<var name="arrival" kind="string" value="" dbName="arrival" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
	<class id="itemout">
		<dbtab name="ITEM_OUT" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="batch" kind="string" value="" dbName="batch" />
		<var name="vt_start" kind="stringDate" value="" dbName="vt_start" />
		<var name="vt_end" kind="stringDate" value="" dbName="vt_end" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
	<class id="country">
		<dbtab name="COUNTRY" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="codealpha2" kind="string" value="" dbName="codealpha2" />
		<var name="codealpha3" kind="string" value="" dbName="codealpha3" />
		<var name="number" kind="string" value="" dbName="number" />
		<var name="enname" kind="string" value="" dbName="enname" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
	<class id="batch">
		<dbtab name="BATCH" />
		<var name="id" kind="int" value="-1" dbName="id" />
		<var name="batch" kind="string" value="" dbName="batch" />
		<var name="vt_start" kind="string" value="" dbName="vt_start" />
		<var name="vt_end" kind="string" value="" dbName="vt_end" />
		<var name="description" kind="string" value="" dbName="description" />
	</class>
</model>