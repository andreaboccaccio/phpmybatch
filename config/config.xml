<!DOCTYPE config>
<config>
<db>
<server>localhost</server>
<username>phpmywhstest</username>
<password>phpmywhstest</password>
<dbname>phpmywhstest</dbname>
</db>
<factories>
<fileName>
config/factories.xml
</fileName>
</factories>
<classes>
<db>mysql</db>
<login>random</login>
</classes>
<login>
<hash>sha512</hash>
</login>
<session>
<persistence>120</persistence>
</session>
<memory>
<rowsPerPage>10</rowsPerPage>
</memory>
<date>
<sqlFormat>%d/%m/%Y</sqlFormat>
<lowerLimitCode>_lld_</lowerLimitCode>
<upperLimitCode>_uld_</upperLimitCode>
<orderCode>_od_</orderCode>
<datesFields>DATA,DATAARRIVO,DATAINIZIOCOMM,DATAFINECOMM</datesFields>
</date>
<model>
<fileName>
config/model.xml
</fileName>
</model>
<sqlQueries>
<fileName>
config/sqlQueries.xml
</fileName>
</sqlQueries>
</config>